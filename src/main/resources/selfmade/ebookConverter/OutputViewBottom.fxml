<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


 <AnchorPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="selfmade.ebookConverter.view.OutputViewBottom">
     <children>
         <HBox prefHeight="103.0" prefWidth="573.0">
             <children>
                 <VBox prefHeight="59.0" prefWidth="591.0">
                     <children>
                         <Button fx:id="createButton" mnemonicParsing="false" onAction="#setCreateButton" prefHeight="26.0" prefWidth="98.0" text="Create File" />
                         <TextField fx:id="createFileTextField" prefHeight="26.0" prefWidth="294.0" promptText="Please enter name...">
                             <VBox.margin>
                                 <Insets top="5.0" />
                             </VBox.margin>
                         </TextField>
                         <Label fx:id="createFileLabel" prefHeight="18.0" prefWidth="304.0" />
                         <Button fx:id="ankiButton" mnemonicParsing="false" text="Anki" />
                     </children>
                 </VBox>
             </children>
         </HBox>
     </children>
 </AnchorPane>
